<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onezero">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".OfflineFireBase"
        android:allowBackup="true"
        android:icon="@drawable/abc_vector_test"
        android:label="@string/app_name"
        android:roundIcon="@drawable/abc_vector_test"
        android:supportsRtl="true"
        android:theme="@style/Theme.Onezero">
        <activity android:name=".Collapsing" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name=".MapsActivity"
            android:label="@string/title_activity_maps" />
        <activity android:name=".Activity_pars_data" />
        <activity
            android:name=".ActivityOne"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ActivityTwo"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ActivityThree"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ActivityFour"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ActivityFive"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ActivitySplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>